<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        let a = 1;
        console.log("Hi+ a + a");
        document.write('Hi + a + a' , "<br>");
        console.log(`Hi + ${a+a}`);
        document.write(`Hi + ${a+a}`, "<br>");

        let b = "sai";
        let c = "  kumar";
        let age = 30;
        let output = `I am ${b + c}, And my age is ${age} years old`;
        document.write(output);
    </script>
</body>
</html>




